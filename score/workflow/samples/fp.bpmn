  <process id="testZayavki" name="Работа с заявкой" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="createDocUT" name="Подготовить заявку" activiti:assignee="${initiator}" activiti:formKey="simpleStatusForm"></userTask>
    <sequenceFlow id="flow3" sourceRef="startevent1" targetRef="createDocUT"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway" default="newToDel"></exclusiveGateway>
