    <sequenceFlow id="newToReview" sourceRef="exclusivegateway1" targetRef="reviewDocUT">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status == "reviewedByBudgetHolder"}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway" default="reviewToDel"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="reviewDocUT" targetRef="exclusivegateway2"></sequenceFlow>
    <userTask id="setFinSources" name="Определить источники финансирования" activiti:assignee="${economist}"></userTask>
