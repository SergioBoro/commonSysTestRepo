      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status == "reviewedByEconomist"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="economistToEnd" sourceRef="exclusivegateway3" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="newToDel" sourceRef="exclusivegateway1" targetRef="endevent2"></sequenceFlow>
    <sequenceFlow id="reviewToNew" sourceRef="exclusivegateway2" targetRef="createDocUT">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status =="new"}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="reviewToDel" sourceRef="exclusivegateway2" targetRef="endevent3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway" default="economistToEnd"></exclusiveGateway>
    <sequenceFlow id="flow13" sourceRef="setFinSources" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="economistToReview" sourceRef="exclusivegateway3" targetRef="reviewDocUT">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status == "reviewedByBudgetHolder"}]]></conditionExpression>
    </sequenceFlow>
